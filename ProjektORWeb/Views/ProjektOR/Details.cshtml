@model ProjektOR

<h1>Dane projektu</h1>
<div class="row">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">@Model.Id @Model.NumerProjektu @Model.Rok</span>
                Index: @Model.Id
                LastName: @Model.NumerProjektu
                Email: @Model.Rok

            </div>
            <div class="card-action">
                <a href="/ProjektOr">Cofnij</a>
                
            </div>
        </div>
    </div>
</div>